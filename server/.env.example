# JSON Web Token secret (replace with a strong random string in .env)
JWT_SECRET=changeme

# Allowed frontend origins (comma-separated if multiple)
CORS_ORIGINS=https://your-frontend-url.onrender.com

# AWS S3 credentials
S3_REGION=us-east-2
S3_BUCKET=gisconnect-evidence-prod
S3_ACCESS_KEY_ID=your-access-key-id
S3_SECRET_ACCESS_KEY=your-secret-access-key

# Link expiration in seconds (e.g., 1 hour = 3600s)
S3_URL_TTL_SECONDS=3600
